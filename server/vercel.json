{
    "version": 2,
    "builds": [
      { "src": "server.js", "use": "@vercel/node" },
      { "src": "wishlist-app/package.json", "use": "@vercel/static-build" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/server.js" },
      { "src": "/(.*)", "dest": "/wishlist-app/$1" }
    ]
  }
  